{
  "manifest_version": 2,
  "name": "React Mirror",
  "description": "Create a React App and mirror it in a Chrome Extension.",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "128": "mirror.png"
  },
  "background": {
    "scripts": ["background.ts"]
},
  "permissions": ["tabs", "activeTab", "storage"]
}
